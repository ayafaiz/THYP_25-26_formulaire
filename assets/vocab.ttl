@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix jdc: <https://monprojet.fr/onto/monvocab#> .

<https://monprojet.fr/onto/monvocab#> a owl:Ontology ;
    dcterms:title "Mon Vocabulaire Projet"@fr .

# Classes
jdc:Utilisateur a rdfs:Class ;
    rdfs:label "Utilisateur"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:Document a rdfs:Class ;
    rdfs:label "Document"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:Projet a rdfs:Class ;
    rdfs:label "Projet"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:Evenement a rdfs:Class ;
    rdfs:label "Événement"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:Lieu a rdfs:Class ;
    rdfs:label "Lieu"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

# Propriétés
jdc:auteur a rdf:Property ;
    rdfs:label "Auteur"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:dateCreation a rdf:Property ;
    rdfs:label "Date de création"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:estLieA a rdf:Property ;
    rdfs:label "Est lié à"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

jdc:description a rdf:Property ;
    rdfs:label "Description"@fr ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .
